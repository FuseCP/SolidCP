        <!--<PackageReference Include="Microsoft.Data.SqlClient.SNI" GeneratePathProperty="true" Version="5.2.0" />-->
        <!--<PackageReference Include="Microsoft.Data.SqlClient.SNI.runtime" GeneratePathProperty="true" Version="5.2.0" />-->
        <!--<PackageReference Include="ZstdSharp.Port" Version="0.8.0" />-->
    </ItemGroup>
    <ItemGroup Condition="$(EmbeddReferences) == 'true'">
        <PackageReference Include="PortableCostura.Fody" Version="6.0.2">
            <PrivateAssets>all</PrivateAssets>
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive; compile</IncludeAssets>
        </PackageReference>
        <PackageReference Include="Fody" Version="6.9.1">
            <PrivateAssets>all</PrivateAssets>
            <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
        </PackageReference>
    </ItemGroup>
    <PropertyGroup Label="SqlClientNativeLibraries" Condition="$(EmbeddReferences) == 'true'">
      <CopySNIFiles>false</CopySNIFiles>
    </PropertyGroup>
    <Target Name="EmbedSqlClientNativeLibraries" BeforeTargets="ResolveReferences" Condition="$(EmbeddReferences) == 'true'">
        <ItemGroup>
            <EmbeddedResource Include="@(SNIDllFiles)">
                <Visible>true</Visible>
                <Link Condition="$([MSBuild]::ValueOrDefault('%(Identity)', '').Contains('x86'))">costura_win_x86\%(Filename)%(Extension)</Link>
                <Link Condition="$([MSBuild]::ValueOrDefault('%(Identity)', '').Contains('x64'))">costura_win_x64\%(Filename)%(Extension)</Link>
                <Link Condition="$([MSBuild]::ValueOrDefault('%(Identity)', '').Contains('arm64'))">costura_win_arm64\%(Filename)%(Extension)</Link>
            </EmbeddedResource>
        </ItemGroup>
    </Target>
