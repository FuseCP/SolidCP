  </ItemGroup>

  <ItemGroup>
    <Analyzer Include="..\SolidCP.Build\bin\Debug\netstandard2.0\RazorBlade.dll" />
    <!--<Analyzer Include="..\SolidCP.Build\bin\Debug\netstandard2.0\System.Web.Razor.dll" />-->
  </ItemGroup>
</Project>
