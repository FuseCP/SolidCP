<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFrameworks>net48</TargetFrameworks>
    <Nullable>disable</Nullable>
    <EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
    <CompilerGeneratedFilesOutputPath>WebServices</CompilerGeneratedFilesOutputPath>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <LangVersion>11</LangVersion>
    <RunCommand>$(MSBuildExtensionsPath64)\..\IIS Express\iisexpress.exe</RunCommand>
    <RunArguments>/site:server</RunArguments>
    <!--<SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>SolidCP.snk</AssemblyOriginatorKeyFile>-->
  </PropertyGroup>

  <PropertyGroup Condition="'$(TargetFramework)' == 'net7.0'">
    <OutputPath>bin</OutputPath>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <PropertyGroup Condition="'$(TargetFramework)' == 'net6.0'">
    <OutputPath>bin</OutputPath>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <PropertyGroup Condition="'$(TargetFramework)' == 'net48'">
    <OutputPath>bin</OutputPath>
    <OutputType>Library</OutputType>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
    <EnableDefaultCompileItems>false</EnableDefaultCompileItems>
    <ImplicitUsings>disable</ImplicitUsings>
    <RunCommand>$(MSBuildExtensionsPath64)\..\IIS Express\iisexpress</RunCommand>
    <RunArguments>/path:&quot;$(MSBuildProjectDirectory)&quot; /port:9900</RunArguments>
  </PropertyGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net48'">
    <Compile Include="Default.aspx.cs" />
    <Compile Include="Default.aspx.designer.cs" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="WebServices\**\*.*" />
    <None Include="WebServices\**\*.*" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Code\ServerUsernameTokenManager.cs" />
    <Compile Remove="Code\UsernameAssertion.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Images\logo.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Include="AutoDiscovery.asmx.cs" />
    <Compile Include="BlackBerry.asmx.cs" />
    <Compile Include="Code\AutoDiscoveryHelper.cs" />
    <Compile Include="Code\PasswordValidator.cs" />
    <Compile Include="Code\ServerConfiguration.cs" />
    <Compile Include="CRM.asmx.cs" />
    <Compile Include="DatabaseServer.asmx.cs" />
    <Compile Include="DNSServer.asmx.cs" />
    <Compile Include="EnterpriseStorage.asmx.cs" />
    <Compile Include="ExchangeServer.asmx.cs" />
    <Compile Include="FTPServer.asmx.cs" />
    <Compile Include="Global.asax.cs" />
    <Compile Include="HeliconZoo.asmx.cs" />
    <Compile Include="HostedSharePointServer.asmx.cs" />
    <Compile Include="HostedSharePointServerEnt.asmx.cs" />
    <Compile Include="LyncServer.asmx.cs" />
    <Compile Include="MailServer.asmx.cs" />
    <Compile Include="OCSEdgeServer.asmx.cs" />
    <Compile Include="OCSServer.asmx.cs" />
    <Compile Include="OperatingSystem.asmx.cs" />
    <Compile Include="Organizations.asmx.cs" />
    <Compile Include="RemoteDesktopServices.asmx.cs" />
    <Compile Include="ServiceProvider.asmx.cs" />
    <Compile Include="SfBServer.asmx.cs" />
    <Compile Include="SharePointServer.asmx.cs" />
    <Compile Include="SpamExperts.asmx.cs" />
    <Compile Include="StatisticsServer.asmx.cs" />
    <Compile Include="StorageSpaceServices.asmx.cs" />
    <Compile Include="VirtualizationServer.asmx.cs" />
    <Compile Include="VirtualizationServer2012.asmx.cs" />
    <Compile Include="VirtualizationServerForPrivateCloud.asmx.cs" />
    <Compile Include="VirtualizationServerProxmox.asmx.cs" />
    <Compile Include="WebServer.asmx.cs" />
    <Compile Include="WindowsServer.asmx.cs" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net6.0'">
    <None Include="Default.aspx.designer.cs" />
    <Compile Remove="Default.aspx.designer.cs" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net7.0'">
    <None Include="Default.aspx.designer.cs" />
    <Compile Remove="Default.aspx.designer.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Include="Code\ServerUsernameTokenManager.cs" />
    <None Include="Code\UsernameAssertion.cs" />
    <Compile Include="Code\WPIHelper.cs" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Images\logo.png" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\SolidCP.Build\SolidCP.Build.csproj" OutputItemType="Analyzer" ReferenceOutputAssembly="false" />
    <ProjectReference Include="..\SolidCP.Web.Services\SolidCP.Web.Services.csproj" />
    <ProjectReference Include="..\SolidCP.Providers.Base\SolidCP.Providers.Base.csproj" />
    <ProjectReference Include="..\SolidCP.Server.Utils\SolidCP.Server.Utils.csproj" />
