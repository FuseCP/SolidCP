	</configSections>
	<appSettings>
		<add key="SolidCP.HyperV.UseDiskPartClearReadOnlyFlag" value="false" />
		<add key="SolidCP.Exchange.ClearQueryBaseDN" value="false" />
		<add key="SolidCP.Exchange.enableSP2abp" value="false" />
