        <machineKey decryptionKey="F0D47152D2AAB5C5523977649BF531C5D7BF4292DEF4D16C" validationKey="D5F316EBF4AF1041CAF115C7BB743007EF6F08B054A2F2A98CAE100D6E6CB9DA7A7FC72D87744BE06C692D5DEFEB2D2D9E56C43F6DA18A2F98007EE2628442DB" />
        <customErrors mode="Off" />
	</system.web>
	    <location path="bin_dotnet">
        <system.webServer>
            <handlers>
                <add name="DisallowServe" path="*.*" verb="*" type="System.Web.HttpNotFoundHandler" />
                <!-- Return 404 instead of 403 -->
            </handlers>
        </system.webServer>
    </location>
	<system.webServer>
		<modules runAllManagedModulesForAllRequests="true" />
        <!-- hide the bin_dotnet folder -->
        <security>
            <requestFiltering>
                <hiddenSegments>
                    <add segment="bin_dotnet" />
                </hiddenSegments>
                <requestLimits maxAllowedContentLength="4194304" />
            </requestFiltering>
        </security>
        <httpErrors errorMode="Detailed" />
		<!--
        To browse web app root directory during debugging, set the value below to true.
        Set to false before deployment to avoid disclosing web app folder information.
      -->
	</system.webServer>
